{
  "name": "code-to-gist",
  "displayName": "code-to-gist",
  "description": "This VSCode extension facilitates seamless posting of code files from your local VSCode environment to GitHub's Gist service, enabling convenient collaboration.",
  "author": "Shodipo Ayomide",
  "version": "0.0.6",
  "icon": "https://i.ibb.co/gytgdGg/Code-To-Gist-logos.jpg",
  "publisher": "developerayo",
  "engines": {
    "vscode": "^1.69.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-to-gist.createGist"
],

  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-to-gist.createGist",
        "title": "Code-To-Gist: Create GitHub Gist from VSCode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
},
  "dependencies": {
    "@types/vscode": "^1.69.1",
    "axios": "^1.4.0"
  }
}
